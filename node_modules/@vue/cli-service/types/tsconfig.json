{
  "files": [
    "cli-service-test.ts",
    "index.d.ts",
    "ProjectOptions.d.ts"
  ],
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "strictFunctionTypes": true,
    "types": [],
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx"
  ]
}
